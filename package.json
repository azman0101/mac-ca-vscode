{
  "name": "mac-ca-vscode",
  "displayName": "Mac CA VSCode",
  "description": "Make all certificates in Keychain Access available to VS Code extensions",
  "version": "0.2.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "publisher": "azman0101",
  "author": {
    "name": "Azman0101",
    "email": "azman0101@hotmail.com"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azman0101/mac-ca-vscode"
  },
  "bugs": {
    "url": "https://github.com/azman0101/mac-ca-vscode"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
